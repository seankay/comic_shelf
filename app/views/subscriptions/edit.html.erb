<% content_for :dynamic_javascripts do %>
  <%= javascript_include_tag "https://js.stripe.com/v1/" %>
<% end %>

<div class="container">
  <%= render 'shared/error_messages', :object => @subscription %>
  <%= render 'plans/plan_info' %>
  <hr>
  <%= form_for [current_store, @subscription] do |f| %>
    <%= render 'shared/credit_card_form', :f => f %>
    <div class="actions">
      <%= f.submit "Subscribe" %>
    </div>
  <% end %>
</div>
